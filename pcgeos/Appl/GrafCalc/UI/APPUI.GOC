@object GCalcTextClass Rechenfenster = {
    GTXI_maxLength =500;
    GI_attrs = @default & GA_TARGETABLE;
    /*GTXI_attrs = @default & ~GTA_USE_TAB_FOR_NAVIGATION;*/
    ATTR_GEN_TEXT_NEVER_MAKE_SCROLLABLE;
    ATTR_GEN_TEXT_DOES_NOT_ACCEPT_INK;
    GTXI_destination=process;
    ATTR_GEN_TEXT_MULTIPLE_CHAR_ATTR_RUNS = @GCalcCharAttrRuns;
    ATTR_GEN_TEXT_MULTIPLE_PARA_ATTR_RUNS = @GCalcParaAttrRuns;
    HINT_FIXED_SIZE = { 320, 160 ,0};	
