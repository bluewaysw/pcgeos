COMMENT @%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
		Stylus Big Key Keymap
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%@
;
;		Big Key Keyboard defines
;

STYLUS_BK_LETTER_FONT_TYPE	equ	FID_BERKELEY
STYLUS_BK_LETTER_FONT_SIZE	equ	14
STYLUS_BK_WORD_FONT_TYPE	equ	STYLUS_WORD_FONT_TYPE
STYLUS_BK_WORD_FONT_SIZE	equ	STYLUS_WORD_FONT_SIZE


STYLUS_BK_WIDTH		equ	21
STYLUS_BK_HEIGHT	equ	21
STYLUS_BK_ROW_4_HEIGHT	equ	16
STYLUS_BK_LEFT_MARGIN	equ	1

STYLUS_BK_ROW_1_L	equ	STYLUS_BK_LEFT_MARGIN
STYLUS_BK_ROW_1_T	equ	1
STYLUS_BK_ROW_1_B	equ	STYLUS_BK_ROW_1_T + STYLUS_BK_HEIGHT

STYLUS_BK_ROW_2_L	equ	STYLUS_LEFT_MARGIN + 10
STYLUS_BK_ROW_2_T	equ	STYLUS_BK_ROW_1_B
STYLUS_BK_ROW_2_B	equ	STYLUS_BK_ROW_2_T + STYLUS_BK_HEIGHT

STYLUS_BK_ROW_3_L	equ	STYLUS_LEFT_MARGIN
STYLUS_BK_ROW_3_T	equ	STYLUS_BK_ROW_2_B
STYLUS_BK_ROW_3_B	equ	STYLUS_BK_ROW_3_T + STYLUS_BK_HEIGHT

STYLUS_BK_ROW_4_L	equ	STYLUS_LEFT_MARGIN + STYLUS_BK_WIDTH*5
STYLUS_BK_ROW_4_T	equ	STYLUS_BK_ROW_3_B
STYLUS_BK_ROW_4_B	equ	STYLUS_BK_ROW_4_T + STYLUS_BK_ROW_4_HEIGHT


STYLUS_BK_KEYBOARD_WIDTH	equ	STYLUS_BK_LEFT_MARGIN + \
					STYLUS_BK_WIDTH*10 + 1
STYLUS_BK_KEYBOARD_HEIGHT	equ	STYLUS_BK_ROW_4_B + 1


VisKeymapCode segment resource

;
; Definition of the BigKey keymap
;

StylusBigKeyLayout Rectangle <
	STYLUS_BK_ROW_1_L + STYLUS_BK_WIDTH*0, STYLUS_BK_ROW_1_T, 	; q Q
	STYLUS_BK_ROW_1_L + STYLUS_BK_WIDTH*1, STYLUS_BK_ROW_1_B
	>,<
	STYLUS_BK_ROW_1_L + STYLUS_BK_WIDTH*1, STYLUS_BK_ROW_1_T, 	; w W
	STYLUS_BK_ROW_1_L + STYLUS_BK_WIDTH*2, STYLUS_BK_ROW_1_B
	>,<
	STYLUS_BK_ROW_1_L + STYLUS_BK_WIDTH*2, STYLUS_BK_ROW_1_T, 	; e E
	STYLUS_BK_ROW_1_L + STYLUS_BK_WIDTH*3, STYLUS_BK_ROW_1_B
	>,<
	STYLUS_BK_ROW_1_L + STYLUS_BK_WIDTH*3, STYLUS_BK_ROW_1_T, 	; r R
	STYLUS_BK_ROW_1_L + STYLUS_BK_WIDTH*4, STYLUS_BK_ROW_1_B
	>,<
	STYLUS_BK_ROW_1_L + STYLUS_BK_WIDTH*4, STYLUS_BK_ROW_1_T, 	; t T
	STYLUS_BK_ROW_1_L + STYLUS_BK_WIDTH*5, STYLUS_BK_ROW_1_B
	>,<
	STYLUS_BK_ROW_1_L + STYLUS_BK_WIDTH*5, STYLUS_BK_ROW_1_T, 	; y Y
	STYLUS_BK_ROW_1_L + STYLUS_BK_WIDTH*6, STYLUS_BK_ROW_1_B
	>,<
	STYLUS_BK_ROW_1_L + STYLUS_BK_WIDTH*6, STYLUS_BK_ROW_1_T, 	; u U
	STYLUS_BK_ROW_1_L + STYLUS_BK_WIDTH*7, STYLUS_BK_ROW_1_B
	>,<
	STYLUS_BK_ROW_1_L + STYLUS_BK_WIDTH*7, STYLUS_BK_ROW_1_T, 	; i I
	STYLUS_BK_ROW_1_L + STYLUS_BK_WIDTH*8, STYLUS_BK_ROW_1_B
	>,<
	STYLUS_BK_ROW_1_L + STYLUS_BK_WIDTH*8, STYLUS_BK_ROW_1_T, 	; o O
	STYLUS_BK_ROW_1_L + STYLUS_BK_WIDTH*9, STYLUS_BK_ROW_1_B
	>,<
	STYLUS_BK_ROW_1_L + STYLUS_BK_WIDTH*9, STYLUS_BK_ROW_1_T, 	; p P
	STYLUS_BK_ROW_1_L + STYLUS_BK_WIDTH*10,STYLUS_BK_ROW_1_B
	>,<
	STYLUS_BK_ROW_2_L + STYLUS_BK_WIDTH*0, STYLUS_BK_ROW_2_T, 	; a A
	STYLUS_BK_ROW_2_L + STYLUS_BK_WIDTH*1, STYLUS_BK_ROW_2_B
	>,<
	STYLUS_BK_ROW_2_L + STYLUS_BK_WIDTH*1, STYLUS_BK_ROW_2_T, 	; s S
	STYLUS_BK_ROW_2_L + STYLUS_BK_WIDTH*2, STYLUS_BK_ROW_2_B
	>,<
	STYLUS_BK_ROW_2_L + STYLUS_BK_WIDTH*2, STYLUS_BK_ROW_2_T, 	; d D
	STYLUS_BK_ROW_2_L + STYLUS_BK_WIDTH*3, STYLUS_BK_ROW_2_B
	>,<
	STYLUS_BK_ROW_2_L + STYLUS_BK_WIDTH*3, STYLUS_BK_ROW_2_T, 	; f F
	STYLUS_BK_ROW_2_L + STYLUS_BK_WIDTH*4, STYLUS_BK_ROW_2_B
	>,<
	STYLUS_BK_ROW_2_L + STYLUS_BK_WIDTH*4, STYLUS_BK_ROW_2_T, 	; g G
	STYLUS_BK_ROW_2_L + STYLUS_BK_WIDTH*5, STYLUS_BK_ROW_2_B
	>,<
	STYLUS_BK_ROW_2_L + STYLUS_BK_WIDTH*5, STYLUS_BK_ROW_2_T, 	; h H
	STYLUS_BK_ROW_2_L + STYLUS_BK_WIDTH*6, STYLUS_BK_ROW_2_B
	>,<
	STYLUS_BK_ROW_2_L + STYLUS_BK_WIDTH*6, STYLUS_BK_ROW_2_T, 	; j J
	STYLUS_BK_ROW_2_L + STYLUS_BK_WIDTH*7, STYLUS_BK_ROW_2_B
	>,<
	STYLUS_BK_ROW_2_L + STYLUS_BK_WIDTH*7, STYLUS_BK_ROW_2_T, 	; k K
	STYLUS_BK_ROW_2_L + STYLUS_BK_WIDTH*8, STYLUS_BK_ROW_2_B
	>,<
	STYLUS_BK_ROW_2_L + STYLUS_BK_WIDTH*8, STYLUS_BK_ROW_2_T, 	; l L
	STYLUS_BK_ROW_2_L + STYLUS_BK_WIDTH*9, STYLUS_BK_ROW_2_B
	>,<
	STYLUS_BK_ROW_3_L + STYLUS_BK_WIDTH*0, STYLUS_BK_ROW_3_T, 	; - /
	STYLUS_BK_ROW_3_L + STYLUS_BK_WIDTH*1, STYLUS_BK_ROW_3_B
	>,<
	STYLUS_BK_ROW_3_L + STYLUS_BK_WIDTH*1, STYLUS_BK_ROW_3_T, 	; z Z
	STYLUS_BK_ROW_3_L + STYLUS_BK_WIDTH*2, STYLUS_BK_ROW_3_B
	>,<
	STYLUS_BK_ROW_3_L + STYLUS_BK_WIDTH*2, STYLUS_BK_ROW_3_T, 	; x X
	STYLUS_BK_ROW_3_L + STYLUS_BK_WIDTH*3, STYLUS_BK_ROW_3_B
	>,<
	STYLUS_BK_ROW_3_L + STYLUS_BK_WIDTH*3, STYLUS_BK_ROW_3_T, 	; c C
	STYLUS_BK_ROW_3_L + STYLUS_BK_WIDTH*4, STYLUS_BK_ROW_3_B
	>,<
	STYLUS_BK_ROW_3_L + STYLUS_BK_WIDTH*4, STYLUS_BK_ROW_3_T, 	; v V
	STYLUS_BK_ROW_3_L + STYLUS_BK_WIDTH*5, STYLUS_BK_ROW_3_B
	>,<
	STYLUS_BK_ROW_3_L + STYLUS_BK_WIDTH*5, STYLUS_BK_ROW_3_T, 	; b B
	STYLUS_BK_ROW_3_L + STYLUS_BK_WIDTH*6, STYLUS_BK_ROW_3_B
	>,<
	STYLUS_BK_ROW_3_L + STYLUS_BK_WIDTH*6, STYLUS_BK_ROW_3_T, 	; n N
	STYLUS_BK_ROW_3_L + STYLUS_BK_WIDTH*7, STYLUS_BK_ROW_3_B
	>,<
	STYLUS_BK_ROW_3_L + STYLUS_BK_WIDTH*7, STYLUS_BK_ROW_3_T, 	; m M
	STYLUS_BK_ROW_3_L + STYLUS_BK_WIDTH*8, STYLUS_BK_ROW_3_B
	>,<
	STYLUS_BK_ROW_3_L + STYLUS_BK_WIDTH*8, STYLUS_BK_ROW_3_T, 	; , '
	STYLUS_BK_ROW_3_L + STYLUS_BK_WIDTH*9, STYLUS_BK_ROW_3_B
	>,<
	STYLUS_BK_ROW_3_L + STYLUS_BK_WIDTH*9, STYLUS_BK_ROW_3_T, 	; , ?
	STYLUS_BK_ROW_3_L + STYLUS_BK_WIDTH*10,STYLUS_BK_ROW_3_B
	>,<
	STYLUS_BK_LEFT_MARGIN,					 	;Dismiss
	STYLUS_BK_ROW_4_B - STYLUS_PIC_KMP_HEIGHT + 2,
	STYLUS_BK_LEFT_MARGIN + STYLUS_DISMISS_WIDTH,
	STYLUS_BK_ROW_4_B
	>,<
	STYLUS_BK_LEFT_MARGIN+STYLUS_PIC_KMP_L+STYLUS_PIC_KMP_WIDTH*0,	;BigKeys
	STYLUS_BK_ROW_4_B - STYLUS_PIC_KMP_HEIGHT + 1,
	STYLUS_BK_LEFT_MARGIN+STYLUS_PIC_KMP_L+STYLUS_PIC_KMP_WIDTH*1,
	STYLUS_BK_ROW_4_B - 1
	>,<
	STYLUS_BK_LEFT_MARGIN+STYLUS_PIC_KMP_L+STYLUS_PIC_KMP_WIDTH*1,	;SmlKeys
	STYLUS_BK_ROW_4_B - STYLUS_PIC_KMP_HEIGHT + 1,
	STYLUS_BK_LEFT_MARGIN+STYLUS_PIC_KMP_L+STYLUS_PIC_KMP_WIDTH*2,
	STYLUS_BK_ROW_4_B - 1
	>,<
	STYLUS_BK_LEFT_MARGIN+STYLUS_PIC_KMP_L+STYLUS_PIC_KMP_WIDTH*2,	;Numbers
	STYLUS_BK_ROW_4_B - STYLUS_PIC_KMP_HEIGHT + 1,
	STYLUS_BK_LEFT_MARGIN+STYLUS_PIC_KMP_L+STYLUS_PIC_KMP_WIDTH*3,
	STYLUS_BK_ROW_4_B - 1
	>,<
	STYLUS_BK_LEFT_MARGIN+STYLUS_PIC_KMP_L+STYLUS_PIC_KMP_WIDTH*3,	;Punct.
	STYLUS_BK_ROW_4_B - STYLUS_PIC_KMP_HEIGHT + 1,
	STYLUS_BK_LEFT_MARGIN+STYLUS_PIC_KMP_L+STYLUS_PIC_KMP_WIDTH*4,
	STYLUS_BK_ROW_4_B - 1
	>,<
	STYLUS_BK_LEFT_MARGIN+STYLUS_PIC_KMP_L+STYLUS_PIC_KMP_WIDTH*4,	;HWRGrid
	STYLUS_BK_ROW_4_B - STYLUS_PIC_KMP_HEIGHT + 1,
	STYLUS_BK_LEFT_MARGIN+STYLUS_PIC_KMP_L+STYLUS_PIC_KMP_WIDTH*5,
	STYLUS_BK_ROW_4_B - 1
	>,<
	STYLUS_BK_ROW_4_L + STYLUS_BK_WIDTH*0, STYLUS_BK_ROW_4_T, 	; space
	STYLUS_BK_ROW_4_L + STYLUS_BK_WIDTH*2+10, STYLUS_BK_ROW_4_B
	>,<
	STYLUS_BK_ROW_4_L + STYLUS_BK_WIDTH*2+10, STYLUS_BK_ROW_4_T, 	; shift
	STYLUS_BK_ROW_4_L + STYLUS_BK_WIDTH*4, STYLUS_BK_ROW_4_B
	>,<
	STYLUS_BK_ROW_4_L + STYLUS_BK_WIDTH*4, STYLUS_BK_ROW_4_T, 	;Backspc
	STYLUS_BK_ROW_4_L + STYLUS_BK_WIDTH*5, STYLUS_BK_ROW_4_B
	>

VisKeymapCode	ends



