;
; Row offsets:
;
STYLUS_KEY_HALF_WIDTH	equ	8

STYLUS_KEY_WIDTH	equ	STYLUS_KEY_HALF_WIDTH*2
STYLUS_KEY_HEIGHT	equ	STYLUS_KEY_HALF_WIDTH*2

STYLUS_ROW_1_T		equ	1					;numbers
STYLUS_ROW_1_B		equ	STYLUS_ROW_1_T + STYLUS_KEY_HEIGHT

STYLUS_ROW_2_T		equ	STYLUS_ROW_1_T + STYLUS_KEY_HEIGHT	;qwerty
STYLUS_ROW_2_B		equ	STYLUS_ROW_2_T + STYLUS_KEY_HEIGHT

STYLUS_ROW_3_T		equ	STYLUS_ROW_2_T + STYLUS_KEY_HEIGHT	;asdfgh
STYLUS_ROW_3_B		equ	STYLUS_ROW_3_T + STYLUS_KEY_HEIGHT

STYLUS_ROW_4_T		equ	STYLUS_ROW_3_T + STYLUS_KEY_HEIGHT	;zxcvbn
STYLUS_ROW_4_B		equ	STYLUS_ROW_4_T + STYLUS_KEY_HEIGHT

STYLUS_ROW_5_T		equ	STYLUS_ROW_4_T + STYLUS_KEY_HEIGHT	;<space>
STYLUS_ROW_5_B		equ	STYLUS_ROW_5_T + STYLUS_KEY_HEIGHT

; Left and right sides for each row:
;
STYLUS_LEFT_MARGIN	equ	1
STYLUS_ROW_1_L		equ	STYLUS_LEFT_MARGIN
STYLUS_ROW_1_R		equ	STYLUS_ROW_1_L + STYLUS_KEY_WIDTH

STYLUS_ROW_2_L		equ	STYLUS_ROW_1_L + STYLUS_KEY_HALF_WIDTH
STYLUS_ROW_2_R		equ	STYLUS_ROW_2_L + STYLUS_KEY_WIDTH

STYLUS_ROW_3_L		equ	STYLUS_ROW_2_L + STYLUS_KEY_HALF_WIDTH
STYLUS_ROW_3_R		equ	STYLUS_ROW_3_L + STYLUS_KEY_WIDTH

STYLUS_ROW_4_L		equ	STYLUS_ROW_3_L + STYLUS_KEY_HALF_WIDTH
STYLUS_ROW_4_R		equ	STYLUS_ROW_4_L + STYLUS_KEY_WIDTH

STYLUS_ROW_5_L		equ	STYLUS_ROW_4_L + (5 * STYLUS_KEY_WIDTH)

STYLUS_RIGHT_MARGIN	equ	(15 * STYLUS_KEY_WIDTH) + STYLUS_ROW_1_L + 4

STYLUS_ALT_GR_SPACE_WIDTH	equ	STYLUS_KEY_WIDTH*3+STYLUS_KEY_HALF_WIDTH
STYLUS_ALT_GR_KEY_WIDTH		equ	STYLUS_KEY_WIDTH*2
STYLUS_NORMAL_SPACE_WIDTH	equ	STYLUS_ALT_GR_SPACE_WIDTH + \
					STYLUS_ALT_GR_KEY_WIDTH

STYLUS_SYMBOL_SCANCODE		equ	RSHIFT_SCANCODE
STYLUS_INTERNATIONAL_SCANCODE	equ	LCTRL_SCANCODE

KEYBOARD_WIDTH		equ	STYLUS_RIGHT_MARGIN + STYLUS_LEFT_MARGIN
KEYBOARD_HEIGHT		equ	STYLUS_ROW_5_B

KEYBOARD_GUESSTIMATE_OF_FLOATING_KBD_WIDTH	equ	242
FLOATING_KEYBOARD_MAX_HEIGHT	equ	KEYBOARD_HEIGHT + 1
FLOATING_KEYBOARD_MAX_WIDTH	equ	KEYBOARD_WIDTH

KEYBOARD_LETTER_FONT_TYPE			equ	FID_BERKELEY
KEYBOARD_LETTER_FONT_SIZE			equ	10
KEYBOARD_WORD_FONT_TYPE				equ	FID_UNIVERSITY
KEYBOARD_WORD_FONT_SIZE				equ	9


KEYBOARD_KEY_HEIGHT	equ	STYLUS_KEY_HEIGHT
KEYBOARD_LAYOUT_OFFSET	equ	offset StylusKeyboardLayout
KEYBOARD_LAYOUT_LENGTH	equ	length StylusKeyboardLayout
KEYBOARD_DRAW_OUTLINES_ROUTINE_OFFSET	equ	\
			offset VisKeyboardCode:KeyboardDrawStylusKeyOutlines

VisKeyboardCode segment resource

;
; Definition of the regular keyboard
;
StylusKeyboardLayout KeyPic <		
	0,0,							; 01 (escape)
	0,0,
	FALSE
	>,<	
	STYLUS_ROW_1_T, STYLUS_ROW_1_L + STYLUS_KEY_WIDTH*1,	; 02
	STYLUS_ROW_1_B, STYLUS_ROW_1_L + STYLUS_KEY_WIDTH*2,
	TRUE
	>,<
	STYLUS_ROW_1_T, STYLUS_ROW_1_L + STYLUS_KEY_WIDTH*2,	; 03
	STYLUS_ROW_1_B, STYLUS_ROW_1_L + STYLUS_KEY_WIDTH*3,
	TRUE
	>,<
	STYLUS_ROW_1_T, STYLUS_ROW_1_L + STYLUS_KEY_WIDTH*3,	; 04
	STYLUS_ROW_1_B, STYLUS_ROW_1_L + STYLUS_KEY_WIDTH*4,
	TRUE
	>,<
	STYLUS_ROW_1_T, STYLUS_ROW_1_L + STYLUS_KEY_WIDTH*4,	; 05
	STYLUS_ROW_1_B, STYLUS_ROW_1_L + STYLUS_KEY_WIDTH*5,
	TRUE
	>,<
	STYLUS_ROW_1_T, STYLUS_ROW_1_L + STYLUS_KEY_WIDTH*5,	; 06
	STYLUS_ROW_1_B, STYLUS_ROW_1_L + STYLUS_KEY_WIDTH*6,
	TRUE
	>,<
