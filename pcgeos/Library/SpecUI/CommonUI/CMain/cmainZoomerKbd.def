COMMENT @%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
		Zoomer Floating Keyboard
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%@

; Row offsets:
;
ZOOMER_KEY_WIDTH	equ	15			;size of keys
ZOOMER_KEY_HEIGHT	equ	14			;size of keys

ZOOMER_ROW_1_T		equ	5					;numbers
ZOOMER_ROW_1_B		equ	ZOOMER_ROW_1_T + ZOOMER_KEY_HEIGHT

ZOOMER_ROW_2_T		equ	ZOOMER_ROW_1_T + ZOOMER_KEY_HEIGHT	;qwerty
ZOOMER_ROW_2_B		equ	ZOOMER_ROW_2_T + ZOOMER_KEY_HEIGHT

ZOOMER_ROW_3_T		equ	ZOOMER_ROW_2_T + ZOOMER_KEY_HEIGHT	;asdfgh
ZOOMER_ROW_3_B		equ	ZOOMER_ROW_3_T + ZOOMER_KEY_HEIGHT

ZOOMER_ROW_4_T		equ	ZOOMER_ROW_3_T + ZOOMER_KEY_HEIGHT	;zxcvbn
ZOOMER_ROW_4_B		equ	ZOOMER_ROW_4_T + ZOOMER_KEY_HEIGHT

ZOOMER_ROW_5_T		equ	ZOOMER_ROW_4_T + ZOOMER_KEY_HEIGHT	;<space>
ZOOMER_ROW_5_B		equ	ZOOMER_ROW_5_T + ZOOMER_KEY_HEIGHT

; Left and right sides for each row:
;
ZOOMER_LEFT_MARGIN	equ	1
ZOOMER_ROW_1_L		equ	7 + ZOOMER_LEFT_MARGIN
ZOOMER_ROW_1_R		equ	ZOOMER_ROW_1_L + ZOOMER_KEY_WIDTH

ZOOMER_ROW_2_L		equ	ZOOMER_ROW_1_L + 7
ZOOMER_ROW_2_R		equ	ZOOMER_ROW_2_L + ZOOMER_KEY_WIDTH

ZOOMER_ROW_3_L		equ	ZOOMER_ROW_2_L + 7
ZOOMER_ROW_3_R		equ	ZOOMER_ROW_3_L + ZOOMER_KEY_WIDTH

ZOOMER_ROW_4_L		equ	ZOOMER_ROW_3_L + 7
ZOOMER_ROW_4_R		equ	ZOOMER_ROW_4_L + ZOOMER_KEY_WIDTH

ZOOMER_ROW_5_L		equ	ZOOMER_ROW_4_L + 8
ZOOMER_ROW_5_R		equ	ZOOMER_ROW_5_L + ZOOMER_KEY_WIDTH

ZOOMER_MAIN_RIGHT	equ	15 * ZOOMER_KEY_WIDTH + ZOOMER_ROW_1_L

; These values are used to force the VisContent which contains
; the keyboard and char tables to be big enough for the keyboard,
; if it happens to be bigger than the char tables.
;
ZOOMER_KBD_WIDTH	equ ZOOMER_MAIN_RIGHT + ZOOMER_LEFT_MARGIN
ZOOMER_KBD_HEIGHT	equ ZOOMER_ROW_5_B

ZOOMER_GUESSTIMATE_OF_FLOATING_KBD_WIDTH	equ	242


if ZOOMER_KEYBOARD
KEYBOARD_KEY_HEIGHT	equ	ZOOMER_KEY_HEIGHT
KEYBOARD_ROW_1_T	equ	ZOOMER_ROW_1_T
KEYBOARD_ROW_2_T	equ	ZOOMER_ROW_2_T
KEYBOARD_ROW_3_T	equ	ZOOMER_ROW_3_T
KEYBOARD_ROW_4_T	equ	ZOOMER_ROW_4_T
KEYBOARD_ROW_5_T	equ	ZOOMER_ROW_5_T
KEYBOARD_ROW_5_B	equ	ZOOMER_ROW_5_B
KEYBOARD_LEFT_MARGIN	equ	ZOOMER_LEFT_MARGIN
KEYBOARD_MAIN_RIGHT	equ	ZOOMER_MAIN_RIGHT
KEYBOARD_GUESSTIMATE_OF_FLOATING_KBD_WIDTH equ \
			ZOOMER_GUESSTIMATE_OF_FLOATING_KBD_WIDTH
KEYBOARD_WIDTH		equ	ZOOMER_KBD_WIDTH
KEYBOARD_HEIGHT		equ	ZOOMER_KBD_HEIGHT
KEYBOARD_LAYOUT_OFFSET	equ	offset ZoomerKeyboardLayout
KEYBOARD_LAYOUT_LENGTH	equ	length ZoomerKeyboardLayout
KEYBOARD_DRAW_OUTLINES_ROUTINE_OFFSET		\
		equ	offset VisKeyboardCode:KeyboardDrawStdOrZmrKeyOutlines
endif		; if ZOOMER_KEYBOARD


;
; Definition of the keyboard itself
;
VisKeyboardCode segment resource

