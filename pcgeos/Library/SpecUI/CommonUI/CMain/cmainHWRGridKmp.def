VisKeymapCode segment resource

;
; Definition of the BigKey keymap
;

StylusHWRGridLayout Rectangle <
	0,2, STYLUS_DISMISS_WIDTH, STYLUS_DISMISS_HEIGHT	; Dismiss
	>,<
	STYLUS_PIC_KMP_L + STYLUS_PIC_KMP_WIDTH*0, 1,		; BigKeys
	STYLUS_PIC_KMP_L + STYLUS_PIC_KMP_WIDTH*1, STYLUS_PIC_KMP_HEIGHT-1
	>,<
	STYLUS_PIC_KMP_L + STYLUS_PIC_KMP_WIDTH*1, 1,		; Keyboard
	STYLUS_PIC_KMP_L + STYLUS_PIC_KMP_WIDTH*2, STYLUS_PIC_KMP_HEIGHT-1
	>,<
	STYLUS_PIC_KMP_L + STYLUS_PIC_KMP_WIDTH*2, 1,		; Numbers
	STYLUS_PIC_KMP_L + STYLUS_PIC_KMP_WIDTH*3, STYLUS_PIC_KMP_HEIGHT-1
	>,<
	STYLUS_PIC_KMP_L + STYLUS_PIC_KMP_WIDTH*3, 1,		; Punctuation
	STYLUS_PIC_KMP_L + STYLUS_PIC_KMP_WIDTH*4, STYLUS_PIC_KMP_HEIGHT-1
	>,<
	STYLUS_PIC_KMP_L + STYLUS_PIC_KMP_WIDTH*4, 1,		; HWRGrid
	STYLUS_PIC_KMP_L + STYLUS_PIC_KMP_WIDTH*5, STYLUS_PIC_KMP_HEIGHT-1
	>

VisKeymapCode ends
