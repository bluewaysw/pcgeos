/* HTML Menu Bar */
@object GenInteractionClass HTMLMenu2 = {
    GI_attrs = @default | GA_KBD_SEARCH_PATH;
GI_comp = @LinksAndGraphicsItems,
            @DividersItems,
            @SpecialCharactersItems,
            @MiscellaneousItems,
            @HTMLFrames /*, @HTMLJavaScript*/ ;
    GI_states = @default & ~GS_USABLE;
    GII_visibility = GIV_POPOUT;
