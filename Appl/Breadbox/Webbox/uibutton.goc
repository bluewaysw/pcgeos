@extern method SampleColorClass, MSG_VIS_RECALC_SIZE
{
  return MAKE_SIZE_DWORD(80, 100);
}

@extern method SampleColorClass, MSG_VIS_DRAW
{
  word carg, red, green, blue, fred, fgreen, fblue, sred, sgreen, sblue;
  VisInstance *vself = ObjDerefVis(oself);

carg = @call /*GeodeGetOptrNS(@*/CSel/*)*/::MSG_GEN_ITEM_GROUP_GET_SELECTION();
if (carg!=GIGS_NONE){
         if (carg==0)
                {
  red = @call /*GeodeGetOptrNS(@*/RedSlider/*)*/::MSG_GEN_VALUE_GET_INTEGER_VALUE();
  green = @call /*GeodeGetOptrNS(@*/GreenSlider/*)*/::MSG_GEN_VALUE_GET_INTEGER_VALUE();
  blue = @call /*GeodeGetOptrNS(@*/BlueSlider/*)*/::MSG_GEN_VALUE_GET_INTEGER_VALUE();
  GrSetAreaColor(gstate, CF_RGB, red, green, blue);
/*  GrSetMixMode(gstate, MM_AND);  helps but leads to black GB - new 4/27/97 */
  GrFillRect(gstate, vself->VI_bounds.R_left+3,
    vself->VI_bounds.R_top+3, vself->VI_bounds.R_right-3,
    vself->VI_bounds.R_bottom-3);
                }
        if (carg==1)
        {
  fred = @call /*GeodeGetOptrNS(@*/RedSlider/*)*/::MSG_GEN_VALUE_GET_INTEGER_VALUE();
  fgreen = @call /*GeodeGetOptrNS(@*/GreenSlider/*)*/::MSG_GEN_VALUE_GET_INTEGER_VALUE();
  fblue = @call /*GeodeGetOptrNS(@*/BlueSlider/*)*/::MSG_GEN_VALUE_GET_INTEGER_VALUE();


        }
}

sred = fred;
sgreen = fgreen;
sblue = fblue;
  GrSetMixMode(gstate, MM_COPY);
/*  GSSetFont(0xd022,14.0), code has no effect */
/*   GSSetFont(MarkerFeltThin=53282,14.0), wrong */
  GrSetTextColor(gstate, CF_RGB, sred, sgreen, sblue);
  GrDrawText(gstate, vself->VI_bounds.R_left+12,
        vself->VI_bounds.R_top+45, "Text", 0);


/*  GrSetTextColor(gstate, CF_INDEX, C_BLACK, 0,0);*/
  GrSetLineColor(gstate, CF_INDEX, C_DARK_GRAY, 0,0);
  GrSetLineWidth(gstate, MakeWWFixed(2));
  GrDrawRect(gstate, vself->VI_bounds.R_left,
    vself->VI_bounds.R_top, vself->VI_bounds.R_right,
    vself->VI_bounds.R_bottom);
  GrSetLineColor(gstate, CF_INDEX, C_WHITE, 0,0);
  GrSetLineWidth(gstate, MakeWWFixed(1));
  GrDrawVLine(gstate, vself->VI_bounds.R_right-1,
    vself->VI_bounds.R_top+1,
    vself->VI_bounds.R_bottom-2);
  GrDrawVLine(gstate, vself->VI_bounds.R_right-2,
    vself->VI_bounds.R_top+2,
    vself->VI_bounds.R_bottom-2);
  GrDrawHLine(gstate, vself->VI_bounds.R_left+1,
    vself->VI_bounds.R_bottom-1,
    vself->VI_bounds.R_right-1);
  GrDrawHLine(gstate, vself->VI_bounds.R_left+2,
    vself->VI_bounds.R_bottom-2,
    vself->VI_bounds.R_right-1);
}

@extern method HTMProcessClass, MSG_COLOR_CHANGED
{
char hex[7];
optr disp;
word count, carg, red, green, blue/*, fred, fgreen, fblue, sred, sgreen, sblue*/;

disp = /*GeodeGetOptrNS(*/@SampleDisplay/*)*/;
@send disp::MSG_VIS_REDRAW_ENTIRE_OBJECT();

red   = @call /*GeodeGetOptrNS(@*/RedSlider/*)*/::MSG_GEN_VALUE_GET_INTEGER_VALUE();
green = @call /*GeodeGetOptrNS(@*/GreenSlider/*)*/::MSG_GEN_VALUE_GET_INTEGER_VALUE();
blue  = @call /*GeodeGetOptrNS(@*/BlueSlider/*)*/::MSG_GEN_VALUE_GET_INTEGER_VALUE();

hex[0] = (red & 0xF0) >>4;
hex[1] = (red & 0x0F);
hex[2] = (green & 0xF0) >>4;
hex[3] = (green & 0x0F);
hex[4] = (blue & 0xF0) >>4;
hex[5] = (blue & 0x0F);
for (count = 0; count <6; count++) {
        if (hex[count]<10){
        hex[count] += ('1'-1);}
        else {
        hex[count] += ('A'-10);}
        }
        hex[6] = 0;

carg = @call /*GeodeGetOptrNS(@*/CSel/*)*/::MSG_GEN_ITEM_GROUP_GET_SELECTION();
if (carg!=GIGS_NONE)    {
         if (carg==0)
                {
/*@call GeodeGetOptrNS(@ForeHex)::MSG_GEN_REPLACE_VIS_MONIKER_TEXT(hex,VUM_DELAYED_VIA_APP_QUEUE);
*/
@call /*GeodeGetOptrNS(@*/ForeHex/*)*/::MSG_VIS_TEXT_REPLACE_ALL_PTR(hex,0);
                }
else  if (carg==1) {
/*@call GeodeGetOptrNS(@BackHex)::MSG_GEN_REPLACE_VIS_MONIKER_TEXT(hex,VUM_DELAYED_VIA_APP_QUEUE);
*/
@call /*GeodeGetOptrNS(@*/BackHex/*)*/::MSG_VIS_TEXT_REPLACE_ALL_PTR(hex,0);
                }
                        }
}


