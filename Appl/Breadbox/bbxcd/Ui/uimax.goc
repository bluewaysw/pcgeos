/***********************************************************************
 *
 * MESSAGE:	MSG_META_START_SELECT for BCTriggerClass
 *
 * PARAMETERS:
 *
 *
 * DESCRIPTION:	sets instance data in BCTriggerClass to get ready
 *		for a possibly held down button
 *
 *
 *
 ***********************************************************************/
@extern method BCTriggerClass, MSG_META_START_SELECT
{
    pself->BCTI_wait = 10 ;
    pself->BCTI_held = TRUE ;
    @send oself::MSG_BC_TRIGGER_CLICK() ;
    if (oself == @MaxRwTrigger)
      searchDirection = TRUE;       // set rewind mode
    if (oself == @MaxFwTrigger)
      searchDirection = FALSE;      // set forward mode
    @callsuper() ;
}

/***********************************************************************
 *
 * MESSAGE:	MSG_BC_TRIGGER_CLICK for BCTriggerClass
 *
 * PARAMETERS:
 *
 *
 * DESCRIPTION:	repeats trigger action if button held down
 *              ( sets the search flag and destination flag every 1/6 second)
 *
 *
 ***********************************************************************/
@extern method BCTriggerClass, MSG_BC_TRIGGER_CLICK
{
  if (pself->BCTI_held == TRUE)  {
    pself->BCTI_timer = TimerStart(
				TIMER_EVENT_ONE_SHOT,
				oself,
				pself->BCTI_wait,
				MSG_BC_TRIGGER_CLICK,
				pself->BCTI_wait,
				&pself->BCTI_timerId);
    searching = 2; // 2 to ensure proper function even if system busy
  }
}

/***********************************************************************
 *
 * MESSAGE:	MSG_META_LOST_MOUSE_EXCL for BCTriggerClass
 *
 * PARAMETERS:
 *
 *
 * DESCRIPTION:	resets instance data in BCTriggerClass
 *
 *
 *
 ***********************************************************************/
@extern method BCTriggerClass, MSG_META_LOST_MOUSE_EXCL
{
    pself->BCTI_held = FALSE ;
    searching = 0;
    @callsuper() ;
}
