@object GenApplicationClass ICApp = {
	GI_visMoniker = list { @ICTextMoniker,
	@Moniker0,@Moniker1,@Moniker2,@Moniker3,@Moniker4,@Moniker5 };
	GI_comp = @ICPrimary;
	gcnList(MANUFACTURER_ID_GEOWORKS,GAGCNLT_WINDOWS) = @ICPrimary;
	gcnList(MANUFACTURER_ID_GEOWORKS,MGCNLT_ACTIVE_LIST) = @ICDocumentControl;
	gcnList(MANUFACTURER_ID_GEOWORKS,MGCNLT_APP_STARTUP) = @ICDocumentControl;
	gcnList(MANUFACTURER_ID_GEOWORKS,GAGCNLT_SELF_LOAD_OPTIONS) =
							@ICDocumentControl;
