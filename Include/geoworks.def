COMMENT @----------------------------------------------------------------------

DESCRIPTION:	Transfer item types

------------------------------------------------------------------------------@

; predefined formats for MANUFACTURER_ID_GEOWORKS

ClipboardItemFormat etype word
CIF_TEXT 		enum	ClipboardItemFormat
	; Null terminated text with formatting information.
	; (see TextTransferBlockHeader documentation in Objects/vTextC.def)

CIF_GRAPHICS_STRING 	enum	ClipboardItemFormat
	; Standard GEOS graphics string
	; (see Graphics String documentation for format -- GST_VMEM)

CIF_FILES 		enum	ClipboardItemFormat
	; Internal desktop format for direct-manipulation file operations
	; (only valid for quick-transfer operations)
	; 			** NOTE **
	;	This format passes a FileQuickTransfer block, the header and
	; content structures of which are defined in .Include/fmtool.def
	;	When using this format, make sure you pass the true diskhandle
	; of the transfer's files (the disk on which the directory containing
	; these files resides after all links have been resolved (use 
	; FileConstructActualPath)) in the CIFI_extra1 field.  CIFI_extra2 is
	; the remote flag, which should be set if ANY file in the transfer
	; is remote (if the FEA_PATH_INFO data doesn't have the
	; DPI_EXISTS_LOCALLY flag set).

CIF_SPREADSHEET 	enum	ClipboardItemFormat

CIF_INK			enum	ClipboardItemFormat
	; 

CIF_GROBJ		enum	ClipboardItemFormat
	; Format specific to the Graphic Object Library.

CIF_GEODEX		enum	ClipboardItemFormat

CIF_BITMAP		enum	ClipboardItemFormat
	; A huge bitmap. See GrCreateBitmap for more details on a huge bitmap.

CIF_SOUND_SYNTH		enum	ClipboardItemFormat
	; A multi-track description of a sound, musical in nature.

CIF_SOUND_SAMPLE	enum	ClipboardItemFormat
	; Sampled sound description.

CIF_ICON_LIST		enum	ClipboardItemFormat
	; Internal icon editor format for transferring groups
	; of icons between icon files.

CIF_FAX_FILE_PAGE_WITH_INK	enum	ClipboardItemFormat
