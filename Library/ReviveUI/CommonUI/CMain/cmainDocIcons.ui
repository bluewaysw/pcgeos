#define DOC_ENTRY_GSTRING(bmap) \
	gstring { \
		GSBeginString | \
		byte	GR_DRAW_BITMAP_OPTR | \
		word	0, 0 | \
		optr	bmap | \
		GSEndString \
	}

#define DOC_ENTRY_BW_GSTRING(bmap) \
	gstring { \
		GSBeginString | \
		byte	GR_FILL_BITMAP_OPTR | \
		word	0, 0 | \
		optr	bmap | \
		GSEndString \
	}

#define DOC_SC_ENTRY_VALUES(bmap) \
	style = icon; \
	size = standard; \
	color = color4; \
	aspectRatio = normal; \
	cachedSize = 64, 40; \
	DOC_ENTRY_GSTRING(bmap)

#define DOC_SM_ENTRY_VALUES(bmap) \
	style = icon; \
	size = standard; \
	color = gray1; \
	aspectRatio = normal; \
	cachedSize = 64, 40; \
	DOC_ENTRY_BW_GSTRING(bmap)

#define DOC_SCGA_ENTRY_VALUES(bmap) \
	style = icon; \
	size = standard; \
	color = gray1; \
	aspectRatio = verySquished; \
	cachedSize = 64, 20; \
	DOC_ENTRY_BW_GSTRING(bmap)

