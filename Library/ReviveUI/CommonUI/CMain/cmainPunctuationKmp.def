COMMENT @%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
		Stylus Punctuation Keyboard
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%@
;
;		Punctuation Keyboard defines
;

STYLUS_PUN_LETTER_FONT_TYPE	equ	FID_BERKELEY
STYLUS_PUN_LETTER_FONT_SIZE	equ	10
STYLUS_PUN_WORD_FONT_TYPE	equ	STYLUS_WORD_FONT_TYPE
STYLUS_PUN_WORD_FONT_SIZE	equ	STYLUS_WORD_FONT_SIZE


STYLUS_PUN_WIDTH	equ	16
STYLUS_PUN_HEIGHT	equ	16
STYLUS_PUN_LEFT_MARGIN	equ	1

STYLUS_PUN_ROW_1_L	equ	STYLUS_PUN_LEFT_MARGIN
STYLUS_PUN_ROW_1_T	equ	1
STYLUS_PUN_ROW_1_B	equ	STYLUS_PUN_ROW_1_T + STYLUS_PUN_HEIGHT

STYLUS_PUN_ROW_2_L	equ	STYLUS_LEFT_MARGIN + 10 + STYLUS_PUN_WIDTH*6
STYLUS_PUN_ROW_2_T	equ	STYLUS_PUN_ROW_1_B
STYLUS_PUN_ROW_2_B	equ	STYLUS_PUN_ROW_2_T + STYLUS_PUN_HEIGHT



STYLUS_PUN_RIGHT_MARGIN	equ	STYLUS_PUN_LEFT_MARGIN + STYLUS_PUN_WIDTH*14
STYLUS_PUN_BOTTOM_MARGIN	equ	STYLUS_PUN_ROW_2_B


VisKeymapCode segment resource

;
; Definition of the Punctuation keymap
;

StylusPunctuationLayout Rectangle <
	STYLUS_PUN_ROW_1_L + STYLUS_PUN_WIDTH*0, STYLUS_PUN_ROW_1_T,	; ,
	STYLUS_PUN_ROW_1_L + STYLUS_PUN_WIDTH*1, STYLUS_PUN_ROW_1_B
	>,<
	STYLUS_PUN_ROW_1_L + STYLUS_PUN_WIDTH*1, STYLUS_PUN_ROW_1_T,	; .
	STYLUS_PUN_ROW_1_L + STYLUS_PUN_WIDTH*2, STYLUS_PUN_ROW_1_B
	>,<
	STYLUS_PUN_ROW_1_L + STYLUS_PUN_WIDTH*2, STYLUS_PUN_ROW_1_T,	; ?
	STYLUS_PUN_ROW_1_L + STYLUS_PUN_WIDTH*3, STYLUS_PUN_ROW_1_B
	>,<
	STYLUS_PUN_ROW_1_L + STYLUS_PUN_WIDTH*3, STYLUS_PUN_ROW_1_T,	; !
	STYLUS_PUN_ROW_1_L + STYLUS_PUN_WIDTH*4, STYLUS_PUN_ROW_1_B
	>,<
	STYLUS_PUN_ROW_1_L + STYLUS_PUN_WIDTH*4, STYLUS_PUN_ROW_1_T,	; (
	STYLUS_PUN_ROW_1_L + STYLUS_PUN_WIDTH*5, STYLUS_PUN_ROW_1_B
	>,<
	STYLUS_PUN_ROW_1_L + STYLUS_PUN_WIDTH*5, STYLUS_PUN_ROW_1_T,	; )
	STYLUS_PUN_ROW_1_L + STYLUS_PUN_WIDTH*6, STYLUS_PUN_ROW_1_B
	>,<
	STYLUS_PUN_ROW_1_L + STYLUS_PUN_WIDTH*6, STYLUS_PUN_ROW_1_T,	; '
	STYLUS_PUN_ROW_1_L + STYLUS_PUN_WIDTH*7, STYLUS_PUN_ROW_1_B
	>,<
	STYLUS_PUN_ROW_1_L + STYLUS_PUN_WIDTH*7, STYLUS_PUN_ROW_1_T,	; "
	STYLUS_PUN_ROW_1_L + STYLUS_PUN_WIDTH*8, STYLUS_PUN_ROW_1_B
	>,<
	STYLUS_PUN_ROW_1_L + STYLUS_PUN_WIDTH*8, STYLUS_PUN_ROW_1_T,	; -
	STYLUS_PUN_ROW_1_L + STYLUS_PUN_WIDTH*9, STYLUS_PUN_ROW_1_B
	>,<
	STYLUS_PUN_ROW_1_L + STYLUS_PUN_WIDTH*9, STYLUS_PUN_ROW_1_T,	; /
	STYLUS_PUN_ROW_1_L + STYLUS_PUN_WIDTH*10, STYLUS_PUN_ROW_1_B
	>,<
	STYLUS_PUN_ROW_1_L + STYLUS_PUN_WIDTH*10, STYLUS_PUN_ROW_1_T,	; ;
	STYLUS_PUN_ROW_1_L + STYLUS_PUN_WIDTH*11, STYLUS_PUN_ROW_1_B
	>,<
	STYLUS_PUN_ROW_1_L + STYLUS_PUN_WIDTH*11, STYLUS_PUN_ROW_1_T,	; :
	STYLUS_PUN_ROW_1_L + STYLUS_PUN_WIDTH*12, STYLUS_PUN_ROW_1_B
	>,<
	STYLUS_PUN_ROW_1_L + STYLUS_PUN_WIDTH*12, STYLUS_PUN_ROW_1_T,	; &
	STYLUS_PUN_ROW_1_L + STYLUS_PUN_WIDTH*13, STYLUS_PUN_ROW_1_B
	>,<
	STYLUS_PUN_ROW_1_L + STYLUS_PUN_WIDTH*13, STYLUS_PUN_ROW_1_T,	; *
	STYLUS_PUN_ROW_1_L + STYLUS_PUN_WIDTH*14, STYLUS_PUN_ROW_1_B
	>,<
	STYLUS_PUN_ROW_2_L + STYLUS_PUN_WIDTH*0, STYLUS_PUN_ROW_2_T,	; Space
	STYLUS_PUN_ROW_2_L + STYLUS_PUN_WIDTH*4, STYLUS_PUN_ROW_2_B
	>,<
	STYLUS_PUN_ROW_2_L + STYLUS_PUN_WIDTH*4, STYLUS_PUN_ROW_2_T,	; Enter
	STYLUS_PUN_ROW_2_L + STYLUS_PUN_WIDTH*6, STYLUS_PUN_ROW_2_B
	>,<
	STYLUS_PUN_ROW_2_L + STYLUS_PUN_WIDTH*6, STYLUS_PUN_ROW_2_T,	;Backspc
	STYLUS_PUN_RIGHT_MARGIN, STYLUS_PUN_ROW_2_B
	>,<
	STYLUS_PUN_LEFT_MARGIN,					 	;Dismiss
	STYLUS_PUN_ROW_2_B - STYLUS_PIC_KMP_HEIGHT + 2,
	STYLUS_PUN_LEFT_MARGIN + STYLUS_DISMISS_WIDTH,
	STYLUS_PUN_ROW_2_B
	>,<
	STYLUS_PUN_LEFT_MARGIN+STYLUS_PIC_KMP_L+STYLUS_PIC_KMP_WIDTH*0,	;BigKeys
	STYLUS_PUN_ROW_2_B - STYLUS_PIC_KMP_HEIGHT + 1,
	STYLUS_PUN_LEFT_MARGIN+STYLUS_PIC_KMP_L+STYLUS_PIC_KMP_WIDTH*1,
	STYLUS_PUN_ROW_2_B - 1
	>,<
	STYLUS_PUN_LEFT_MARGIN+STYLUS_PIC_KMP_L+STYLUS_PIC_KMP_WIDTH*1,	;SmlKeys
	STYLUS_PUN_ROW_2_B - STYLUS_PIC_KMP_HEIGHT + 1,
	STYLUS_PUN_LEFT_MARGIN+STYLUS_PIC_KMP_L+STYLUS_PIC_KMP_WIDTH*2,
	STYLUS_PUN_ROW_2_B - 1
	>,<
	STYLUS_PUN_LEFT_MARGIN+STYLUS_PIC_KMP_L+STYLUS_PIC_KMP_WIDTH*2,	;Numbers
	STYLUS_PUN_ROW_2_B - STYLUS_PIC_KMP_HEIGHT + 1,
	STYLUS_PUN_LEFT_MARGIN+STYLUS_PIC_KMP_L+STYLUS_PIC_KMP_WIDTH*3,
	STYLUS_PUN_ROW_2_B - 1
	>,<
	STYLUS_PUN_LEFT_MARGIN+STYLUS_PIC_KMP_L+STYLUS_PIC_KMP_WIDTH*3,	;Punct.
	STYLUS_PUN_ROW_2_B - STYLUS_PIC_KMP_HEIGHT + 1,
	STYLUS_PUN_LEFT_MARGIN+STYLUS_PIC_KMP_L+STYLUS_PIC_KMP_WIDTH*4,
	STYLUS_PUN_ROW_2_B - 1
	>,<
	STYLUS_PUN_LEFT_MARGIN+STYLUS_PIC_KMP_L+STYLUS_PIC_KMP_WIDTH*4,	;HWRGrid
	STYLUS_PUN_ROW_2_B - STYLUS_PIC_KMP_HEIGHT + 1,
	STYLUS_PUN_LEFT_MARGIN+STYLUS_PIC_KMP_L+STYLUS_PIC_KMP_WIDTH*5,
	STYLUS_PUN_ROW_2_B - 1
	>

VisKeymapCode	ends
