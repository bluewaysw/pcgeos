/* ----- */
chunk	CharTableSymbolsLine1Data = data {
	wchar	"@^~#[]{}()%"
	wchar	"$"
	wchar	C_CENT_SIGN, 0
}
chunk	CharTableSymbolsLine2Data = data {
	wchar	"`"
	wchar	C_POUND_SIGN, C_YEN_SIGN, C_CURRENCY_SIGN, 0
}
