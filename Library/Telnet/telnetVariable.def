;-----------------------
; Telnet control block
;-----------------------
TelnetControl   segment lmem LMEM_TYPE_GENERAL, mask LMF_RETURN_ERRORS

if	ERROR_CHECK
;
; Keeps track of all connections by TelnetConnectionID
;	
TelnetIDArray	chunk   TelnetConnectionID
	TelnetIDArrayHeader \
		<<0, size TelnetConnectionID, 0, size TelnetIDArrayHeader>,0>
TelnetIDArray 	endc
	
endif	; if ERROR_CHECK

TelnetControl   ends

;------------------------
; 	Strings
;------------------------
Strings		segment	lmem LMEM_TYPE_GENERAL, mask LMF_RETURN_ERRORS

if	_CLOSE_MEDIUM
TCPIPText    	chunk   TCHAR
        TCHAR    "TCPIP", 0
TCPIPText 	endc
endif	; _CLOSE_MEDIUM

Strings		ends

