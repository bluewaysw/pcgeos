POINTS_PER_INCH		equ	72

; 1 inch left/right margins
;
DEFAULT_LEFT_MARGIN	equ	POINTS_PER_INCH
DEFAULT_RIGHT_MARGIN	equ	DEFAULT_LEFT_MARGIN

; 1 inch top/bottom margins
;
DEFAULT_TOP_MARGIN	equ	POINTS_PER_INCH
DEFAULT_BOTTOM_MARGIN	equ	DEFAULT_TOP_MARGIN

; Default page is 8.5 x 11 inches
;
DEFAULT_PAGE_WIDTH	equ	(POINTS_PER_INCH * 17 / 2)
DEFAULT_PAGE_HEIGHT	equ	(POINTS_PER_INCH * 11)
