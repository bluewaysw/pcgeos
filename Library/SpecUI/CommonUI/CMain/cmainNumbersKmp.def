COMMENT @%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
		Stylus Numbers Keymap
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%@
;
;		Numbers Keyboard defines
;

STYLUS_NUM_LETTER_FONT_TYPE	equ	FID_BERKELEY
STYLUS_NUM_LETTER_FONT_SIZE	equ	10
STYLUS_NUM_WORD_FONT_TYPE	equ	STYLUS_WORD_FONT_TYPE
STYLUS_NUM_WORD_FONT_SIZE	equ	STYLUS_WORD_FONT_SIZE


STYLUS_NUM_GROUP_PADDING	equ	5
STYLUS_NUM_HEIGHT		equ	16

STYLUS_NUM_A_WIDTH	equ	21
STYLUS_NUM_GROUP_A_L	equ	1

STYLUS_NUM_B_WIDTH	equ	19
STYLUS_NUM_GROUP_B_L	equ	STYLUS_NUM_GROUP_A_L + \
				STYLUS_NUM_A_WIDTH*3 + \
				STYLUS_NUM_GROUP_PADDING

STYLUS_NUM_C_WIDTH	equ	19
STYLUS_NUM_GROUP_C_L	equ	STYLUS_NUM_GROUP_B_L + \
				STYLUS_NUM_B_WIDTH*2 + \
				STYLUS_NUM_GROUP_PADDING

STYLUS_NUM_RIGHT_MARGIN	equ	STYLUS_NUM_GROUP_C_L + \
				STYLUS_NUM_C_WIDTH*2
STYLUS_NUM_LEFT_MARGIN	equ	1

STYLUS_NUM_ROW_1_T	equ	STYLUS_NUM_LEFT_MARGIN
STYLUS_NUM_ROW_1_B	equ	STYLUS_NUM_ROW_1_T + STYLUS_NUM_HEIGHT

STYLUS_NUM_ROW_2_T	equ	STYLUS_NUM_ROW_1_B
STYLUS_NUM_ROW_2_B	equ	STYLUS_NUM_ROW_2_T + STYLUS_NUM_HEIGHT

STYLUS_NUM_ROW_3_T	equ	STYLUS_NUM_ROW_2_B
STYLUS_NUM_ROW_3_B	equ	STYLUS_NUM_ROW_3_T + STYLUS_NUM_HEIGHT

STYLUS_NUM_ROW_4_T	equ	STYLUS_NUM_ROW_3_B
STYLUS_NUM_ROW_4_B	equ	STYLUS_NUM_ROW_4_T + STYLUS_NUM_HEIGHT

STYLUS_NUM_ROW_5_T	equ	STYLUS_NUM_ROW_4_B
STYLUS_NUM_ROW_5_B	equ	STYLUS_NUM_ROW_5_T + STYLUS_NUM_HEIGHT

;
; Height is an extra row to fit the graphical item group to select keyboards
;
STYLUS_NUM_BOTTOM_MARGIN	equ	STYLUS_NUM_ROW_4_B + STYLUS_NUM_HEIGHT


VisKeymapCode segment resource

;
; Definition of the Numbers keyboard
;

StylusNumbersLayout Rectangle <		
	STYLUS_NUM_GROUP_A_L + STYLUS_NUM_A_WIDTH*0, STYLUS_NUM_ROW_1_T,; 1
	STYLUS_NUM_GROUP_A_L + STYLUS_NUM_A_WIDTH*1, STYLUS_NUM_ROW_1_B
	>,<
	STYLUS_NUM_GROUP_A_L + STYLUS_NUM_A_WIDTH*1, STYLUS_NUM_ROW_1_T,; 2
	STYLUS_NUM_GROUP_A_L + STYLUS_NUM_A_WIDTH*2, STYLUS_NUM_ROW_1_B
	>,<
	STYLUS_NUM_GROUP_A_L + STYLUS_NUM_A_WIDTH*2, STYLUS_NUM_ROW_1_T,; 3
	STYLUS_NUM_GROUP_A_L + STYLUS_NUM_A_WIDTH*3, STYLUS_NUM_ROW_1_B
	>,<
	STYLUS_NUM_GROUP_A_L + STYLUS_NUM_A_WIDTH*0, STYLUS_NUM_ROW_2_T,; 4
	STYLUS_NUM_GROUP_A_L + STYLUS_NUM_A_WIDTH*1, STYLUS_NUM_ROW_2_B
	>,<
	STYLUS_NUM_GROUP_A_L + STYLUS_NUM_A_WIDTH*1, STYLUS_NUM_ROW_2_T,; 5
	STYLUS_NUM_GROUP_A_L + STYLUS_NUM_A_WIDTH*2, STYLUS_NUM_ROW_2_B
	>,<
	STYLUS_NUM_GROUP_A_L + STYLUS_NUM_A_WIDTH*2, STYLUS_NUM_ROW_2_T,; 6
	STYLUS_NUM_GROUP_A_L + STYLUS_NUM_A_WIDTH*3, STYLUS_NUM_ROW_2_B
	>,<
	STYLUS_NUM_GROUP_A_L + STYLUS_NUM_A_WIDTH*0, STYLUS_NUM_ROW_3_T,; 7
	STYLUS_NUM_GROUP_A_L + STYLUS_NUM_A_WIDTH*1, STYLUS_NUM_ROW_3_B
	>,<
	STYLUS_NUM_GROUP_A_L + STYLUS_NUM_A_WIDTH*1, STYLUS_NUM_ROW_3_T,; 8
	STYLUS_NUM_GROUP_A_L + STYLUS_NUM_A_WIDTH*2, STYLUS_NUM_ROW_3_B
	>,<
	STYLUS_NUM_GROUP_A_L + STYLUS_NUM_A_WIDTH*2, STYLUS_NUM_ROW_3_T,; 9
	STYLUS_NUM_GROUP_A_L + STYLUS_NUM_A_WIDTH*3, STYLUS_NUM_ROW_3_B
	>,<
	STYLUS_NUM_GROUP_A_L + STYLUS_NUM_A_WIDTH*0, STYLUS_NUM_ROW_4_T,; (
	STYLUS_NUM_GROUP_A_L + STYLUS_NUM_A_WIDTH*1, STYLUS_NUM_ROW_4_B
	>,<
	STYLUS_NUM_GROUP_A_L + STYLUS_NUM_A_WIDTH*1, STYLUS_NUM_ROW_4_T,; 0
	STYLUS_NUM_GROUP_A_L + STYLUS_NUM_A_WIDTH*2, STYLUS_NUM_ROW_4_B
	>,<
	STYLUS_NUM_GROUP_A_L + STYLUS_NUM_A_WIDTH*2, STYLUS_NUM_ROW_4_T,; )
	STYLUS_NUM_GROUP_A_L + STYLUS_NUM_A_WIDTH*3, STYLUS_NUM_ROW_4_B
	>,<
	STYLUS_NUM_GROUP_B_L + STYLUS_NUM_B_WIDTH*0, STYLUS_NUM_ROW_1_T,; .
	STYLUS_NUM_GROUP_B_L + STYLUS_NUM_B_WIDTH*1, STYLUS_NUM_ROW_1_B
	>,<
	STYLUS_NUM_GROUP_B_L + STYLUS_NUM_B_WIDTH*1, STYLUS_NUM_ROW_1_T,; :
	STYLUS_NUM_GROUP_B_L + STYLUS_NUM_B_WIDTH*2, STYLUS_NUM_ROW_1_B
	>,<
	STYLUS_NUM_GROUP_B_L + STYLUS_NUM_B_WIDTH*0, STYLUS_NUM_ROW_2_T,; -
	STYLUS_NUM_GROUP_B_L + STYLUS_NUM_B_WIDTH*1, STYLUS_NUM_ROW_2_B
	>,<
	STYLUS_NUM_GROUP_B_L + STYLUS_NUM_B_WIDTH*1, STYLUS_NUM_ROW_2_T,; ,
	STYLUS_NUM_GROUP_B_L + STYLUS_NUM_B_WIDTH*2, STYLUS_NUM_ROW_2_B
	>,<
	STYLUS_NUM_GROUP_B_L + STYLUS_NUM_B_WIDTH*0, STYLUS_NUM_ROW_3_T, ; Enter
	STYLUS_NUM_GROUP_B_L + STYLUS_NUM_B_WIDTH*2, STYLUS_NUM_ROW_3_B
	>,<
	STYLUS_NUM_GROUP_B_L + STYLUS_NUM_B_WIDTH*0, STYLUS_NUM_ROW_4_T,; Space
	STYLUS_NUM_GROUP_B_L + STYLUS_NUM_B_WIDTH*2, STYLUS_NUM_ROW_4_B
	>,<
	STYLUS_NUM_GROUP_C_L + STYLUS_NUM_C_WIDTH*0, STYLUS_NUM_ROW_1_T,; *
	STYLUS_NUM_GROUP_C_L + STYLUS_NUM_C_WIDTH*1, STYLUS_NUM_ROW_1_B
	>,<
	STYLUS_NUM_GROUP_C_L + STYLUS_NUM_C_WIDTH*1, STYLUS_NUM_ROW_1_T,; #
	STYLUS_NUM_GROUP_C_L + STYLUS_NUM_C_WIDTH*2, STYLUS_NUM_ROW_1_B
	>,<
	STYLUS_NUM_GROUP_C_L + STYLUS_NUM_C_WIDTH*0, STYLUS_NUM_ROW_2_T,; /
	STYLUS_NUM_GROUP_C_L + STYLUS_NUM_C_WIDTH*1, STYLUS_NUM_ROW_2_B
	>,<
	STYLUS_NUM_GROUP_C_L + STYLUS_NUM_C_WIDTH*1, STYLUS_NUM_ROW_2_T,; @
	STYLUS_NUM_GROUP_C_L + STYLUS_NUM_C_WIDTH*2, STYLUS_NUM_ROW_2_B
	>,<
	STYLUS_NUM_GROUP_C_L + STYLUS_NUM_C_WIDTH*0, STYLUS_NUM_ROW_3_T,; +
	STYLUS_NUM_GROUP_C_L + STYLUS_NUM_C_WIDTH*1, STYLUS_NUM_ROW_3_B
	>,<
	STYLUS_NUM_GROUP_C_L + STYLUS_NUM_C_WIDTH*1, STYLUS_NUM_ROW_3_T,; $
	STYLUS_NUM_GROUP_C_L + STYLUS_NUM_C_WIDTH*2, STYLUS_NUM_ROW_3_B
	>,<
	STYLUS_NUM_GROUP_C_L + STYLUS_NUM_C_WIDTH*0, STYLUS_NUM_ROW_4_T,; =
	STYLUS_NUM_GROUP_C_L + STYLUS_NUM_C_WIDTH*1, STYLUS_NUM_ROW_4_B
	>,<
	STYLUS_NUM_GROUP_C_L + STYLUS_NUM_C_WIDTH*1, STYLUS_NUM_ROW_4_T,; %
	STYLUS_NUM_GROUP_C_L + STYLUS_NUM_C_WIDTH*2, STYLUS_NUM_ROW_4_B
	>,<
	STYLUS_NUM_GROUP_C_L + STYLUS_NUM_C_WIDTH*0, STYLUS_NUM_ROW_5_T,;Backspc
	STYLUS_NUM_GROUP_C_L + STYLUS_NUM_C_WIDTH*2, STYLUS_NUM_ROW_5_B
	>,<
	STYLUS_NUM_LEFT_MARGIN,					 	;Dismiss
	STYLUS_NUM_ROW_5_B - STYLUS_PIC_KMP_HEIGHT + 2,
	STYLUS_NUM_LEFT_MARGIN + STYLUS_DISMISS_WIDTH,
	STYLUS_NUM_ROW_5_B
	>,<
	STYLUS_NUM_LEFT_MARGIN+STYLUS_PIC_KMP_L+STYLUS_PIC_KMP_WIDTH*0,	;BigKeys
	STYLUS_NUM_ROW_5_B - STYLUS_PIC_KMP_HEIGHT + 1,
	STYLUS_NUM_LEFT_MARGIN+STYLUS_PIC_KMP_L+STYLUS_PIC_KMP_WIDTH*1,
	STYLUS_NUM_ROW_5_B - 1
	>,<
	STYLUS_NUM_LEFT_MARGIN+STYLUS_PIC_KMP_L+STYLUS_PIC_KMP_WIDTH*1,	;SmlKeys
	STYLUS_NUM_ROW_5_B - STYLUS_PIC_KMP_HEIGHT + 1,
	STYLUS_NUM_LEFT_MARGIN+STYLUS_PIC_KMP_L+STYLUS_PIC_KMP_WIDTH*2,
	STYLUS_NUM_ROW_5_B - 1
	>,<
	STYLUS_NUM_LEFT_MARGIN+STYLUS_PIC_KMP_L+STYLUS_PIC_KMP_WIDTH*2,	;Numbers
	STYLUS_NUM_ROW_5_B - STYLUS_PIC_KMP_HEIGHT + 1,
	STYLUS_NUM_LEFT_MARGIN+STYLUS_PIC_KMP_L+STYLUS_PIC_KMP_WIDTH*3,
	STYLUS_NUM_ROW_5_B - 1
	>,<
	STYLUS_NUM_LEFT_MARGIN+STYLUS_PIC_KMP_L+STYLUS_PIC_KMP_WIDTH*3,	;Punct.
	STYLUS_NUM_ROW_5_B - STYLUS_PIC_KMP_HEIGHT + 1,
	STYLUS_NUM_LEFT_MARGIN+STYLUS_PIC_KMP_L+STYLUS_PIC_KMP_WIDTH*4,
	STYLUS_NUM_ROW_5_B - 1
	>,<
	STYLUS_NUM_LEFT_MARGIN+STYLUS_PIC_KMP_L+STYLUS_PIC_KMP_WIDTH*4,	;HWRGrid
	STYLUS_NUM_ROW_5_B - STYLUS_PIC_KMP_HEIGHT + 1,
	STYLUS_NUM_LEFT_MARGIN+STYLUS_PIC_KMP_L+STYLUS_PIC_KMP_WIDTH*5,
	STYLUS_NUM_ROW_5_B - 1
	>


VisKeymapCode	ends

